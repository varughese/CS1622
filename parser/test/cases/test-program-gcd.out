[program
  [fun-declaration
    [int]
    [gcd]
    [params 
      [param [int] [u]] 
      [param [int] [v]]]
    [compound-stmt 
      [selection-stmt 
        [== [var [v]] [0]]
        [return-stmt[var [u]]]
        [return-stmt
          [call
            [gcd]
            [args [var [v]] 
              [- [var [u]]
                [* 
                  [/ [var [u]][var [v]]][var [v]]]]]
          ]]
      ]
    ]
  ]
  [fun-declaration
    [void]
    [main]
    [params]
    [compound-stmt 
      [var-declaration [int] [x]]
      [var-declaration [int] [y]]
      [= [var [x]]
        [call
          [input]
          [args]
        ]]
      [= [var [y]]
        [call
          [input]
          [args]
        ]]
      [call
        [output]
        [args 
          [call
            [gcd]
            [args [var [x]] [var [y]]]
          ]]
      ]
    ]
  ]
]
